{
  "name": "@doseofted/strapi-graphql-flatten",
  "version": "0.0.3",
  "description": "Take a Strapi V4 GraphQL response and transform it into one similar to Strapi V3",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup --watch src/index.ts",
    "test": "vitest run",
    "testing": "vitest"
  },
  "author": {
    "name": "Ted Klingenberg",
    "email": "ted@doseofted.com",
    "url": "https://doseofted.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doseofted-llc/strapi-graphql-flatten"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.3.1",
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"]
}
